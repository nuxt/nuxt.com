{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "jsx": "preserve",
    "strict": true,
    "skipDefaultLibCheck": true,
    "types": [
      "nuxt/dist/app/types/augments"
    ],
    "paths": {
      "#imports": [
        "node_modules/nuxt/dist/app"
      ]
    }
  },
  "include": [
    "src"
  ]
}
