{
  "name": "nuxt-website-theme",
  "version": "0.2.2",
  "main": "nuxt.config.ts",
  "files": [
    "app/",
    "assets/",
    "components/",
    "composables/",
    "layouts/",
    "pages/",
    "plugins/",
    "server/",
    "types/",
    "ui/",
    "utils/",
    "app.vue",
    "app.config.ts",
    "error.vue",
    "nuxt.config.ts",
    "tailwind.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "dev": "nuxi prepare && nuxi dev .demo",
    "build": "nuxi prepare && nuxi build .demo",
    "generate": "nuxi prepare && nuxi generate .demo",
    "preview": "nuxi preview .demo",
    "lint": "eslint --ext .ts,.js,.vue .",
    "release": "release-it",
    "test": "yarn lint"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.0",
    "@docsearch/js": "^3.3.0",
    "@nuxt-themes/typography": "^0.1.4",
    "@nuxt/content": "^2.3.0",
    "@nuxthq/studio": "^0.3.4",
    "@nuxthq/ui": "^0.1.17",
    "@nuxtjs/algolia": "^1.3.1",
    "@nuxtjs/fontaine": "^0.2.3",
    "@nuxtjs/html-validator": "^1.2.3",
    "@nuxtlabs/github-module": "^1.5.4",
    "@vueuse/core": "^9.6.0",
    "@vueuse/motion": "^2.0.0-beta.12",
    "joi": "^17.7.0",
    "mjml": "^4.13.0",
    "node-mailjet": "^6.0.0",
    "nuxt-icon": "^0.1.8",
    "nuxt-plausible": "^0.1.2",
    "remove": "^0.1.5",
    "sitemap": "^7.1.1",
    "tailwindcss-border-gradient-radius": "^3.0.1",
    "three": "^0.147.0",
    "vue-lite-youtube-embed": "^1.0.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.29.0",
    "nuxt": "^3.0.0",
    "release-it": "^15.5.1",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "parse5": "6.0.1"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
